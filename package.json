{
  "name": "polymer-starter-kit",
  "version": "4.0.0",
  "description": "A starting point for Polymer apps",
  "author": "The Polymer Authors",
  "license": "BSD-3-Clause",
  "repository": "Polymer/polymer-starter-kit",
  "scripts": {
    "startwatch": "npm run start | npm run watch",
    "watch": "browser-sync start --proxy localhost:8081 --open --startPath \"src\" --files \"**/*.js\"",
    "start": "polymer serve",
    "build": "polymer build",
    "lint": "polymer lint",
    "test": "polymer test",
    "test:integration": "polymer build # test that psk builds without error with the CLI"
  },
  "dependencies": {
    "@polymer/app-layout": "^3.0.0-pre.15",
    "@polymer/app-route": "^3.0.0-pre.15",
    "@polymer/iron-ajax": "^3.0.0-pre.21",
    "@polymer/iron-flex-layout": "^3.0.0-pre.15",
    "@polymer/iron-iconset-svg": "^3.0.0-pre.15",
    "@polymer/iron-image": "^3.0.0-pre.21",
    "@polymer/iron-list": "^3.0.0-pre.21",
    "@polymer/iron-media-query": "^3.0.0-pre.15",
    "@polymer/iron-pages": "^3.0.0-pre.15",
    "@polymer/iron-selector": "^3.0.0-pre.15",
    "@polymer/paper-icon-button": "^3.0.0-pre.15",
    "@polymer/polymer": "^3.0.0",
    "@webcomponents/webcomponentsjs": "^2.0.0",
    "web-component-tester": "^6.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.35",
    "browser-sync": "^2.24.6",
    "chai": "^4.1.2",
    "polymer-cli": "^1.7.1",
    "sinon": "^6.1.4",
    "wct-browser-legacy": "^1.0.0",
    "wct-local": "^2.1.1"
  },
  "engines": {
    "node": ">=8.0"
  }
}
